{
  "name": "@chatbot-proto/chatbot-proto-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@chatbot-proto/chatbot-proto"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/chatbot-proto-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@chatbot-proto/chatbot-proto:build",
          "@chatbot-proto/chatbot-proto:serve"
        ]
      }
    }
  }
}
